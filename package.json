  "scripts": {
    "set-all": "npm run gem-install && npm run npm-update && npm run npm-install-g && npm run gulp-install && npm run bower-install",
    "gem-install": "gem update && gem update --system && gem install sass scss_lint csscss",
    "npm-update": "npm install -g npm",
    "yarn-install": "npm install -g yarn",
    "npm-install-g": "npm install -g gulp-cli pug-cli bower browser-sync jshint eslint",
    "gulp-install": "npm install",
    "bower-install": "bower install && gulp collect-js-files",
    "babel": "babel src/js/es6 --out-dir src/js/es5 --watch",
    "webpack": "webpack --config src/webpack.config.js",
    "npm-outdated": "npm outdated",
    "yarn-outdated": "yarn outdated",
    "node-sass": "node node_modules/node-sass/bin/node-sass src/sass/main.scss src/css/main.css",
    "ncu": "ncu && ncu -g && ncu -m bower"
  },
